.property
    overflow auto
    max-width 100%

    a
        text-decoration underline

        &:hover
            text-decoration underline

    span
        padding-left 10px
        color #99aaae

    input
        border none
        border-bottom 1px solid #d6ddde


@media (prefers-color-scheme: dark)
    .property
        span
            color #697278
